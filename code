#include<iostream>
using namespace std;
int main()
{
    int a,count=0;
    cout<<"give the number"<<endl;
    cin>>a;
    for(int i=1;i<=a;i++)
    {
        if(a%i==0)
        {
            count++;
        }
    }
    if(count++==2)
    {
        cout<<"it is a prime number"<<endl;
    }
    else{
        cout<<"it is not prime number"<<endl;
    }
    return 0;
}
